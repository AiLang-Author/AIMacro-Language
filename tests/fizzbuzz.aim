# fizzbuzz.aim
def fizzbuzz(n):
    i = 1
    while i <= n:
        if i % 15 == 0:
            print("FizzBuzz")
        elif i % 3 == 0:
            print("Fizz")
        elif i % 5 == 0:
            print("Buzz")
        else:
            print(i)
        end
        i += 1
    end
end

def main():
    fizzbuzz(30)
end