# test_min.aim - with explicit main
class Counter:
    def __init__(self):
        self.count = 0
    end
    
    def increment(self):
        self.count = self.count + 1
    end
    
    def get(self):
        return self.count
    end
end

def main():
    c = Counter()
    c.increment()
    c.increment()
    val = c.get()
    print(val)
end