# Minimal parameter debugging test

def test_six(a, b, c, d, e, f):
    print("In test_six:")
    print("a=" + str(a))
    print("b=" + str(b))
    print("c=" + str(c))
    print("d=" + str(d))
    print("e=" + str(e))
    print("f=" + str(f))
    return a + b + c + d + e + f
end

def test_seven(a, b, c, d, e, f, g):
    print("In test_seven:")
    print("a=" + str(a))
    print("b=" + str(b))
    print("c=" + str(c))
    print("d=" + str(d))
    print("e=" + str(e))
    print("f=" + str(f))
    print("g=" + str(g))
    return a + b + c + d + e + f + g
end

def test_list_sum_simple():
    arr = [10, 20, 30]
    
    a = arr[0]
    b = arr[1]
    c = arr[2]
    
    print("a=" + str(a))
    print("b=" + str(b))
    print("c=" + str(c))
    
    total = a + b + c
    print("total=" + str(total))
    
    return total
end

def test_list_sum_direct():
    arr = [10, 20, 30]
    
    x = arr[0]
    print("x=arr[0]=" + str(x))
    
    y = arr[1]
    print("y=arr[1]=" + str(y))
    
    z = arr[2]
    print("z=arr[2]=" + str(z))
    
    step1 = x + y
    print("x+y=" + str(step1))
    
    step2 = step1 + z
    print("x+y+z=" + str(step2))
    
    return step2
end

def test_list_inline_sum():
    arr = [100, 200, 300]
    
    result = arr[0] + arr[1] + arr[2]
    print("Inline sum result: " + str(result))
    
    return result
end

def main():
    print("=== SIX PARAMS ===")
    r6 = test_six(1, 2, 3, 4, 5, 6)
    print("Result: " + str(r6))
    print("Expected: 21")
    print("")
    
    print("=== SEVEN PARAMS ===")
    r7 = test_seven(1, 2, 3, 4, 5, 6, 7)
    print("Result: " + str(r7))
    print("Expected: 28")
    print("")
    
    print("=== LIST SUM SIMPLE ===")
    s1 = test_list_sum_simple()
    print("Result: " + str(s1))
    print("Expected: 60")
    print("")
    
    print("=== LIST SUM DIRECT ===")
    s2 = test_list_sum_direct()
    print("Result: " + str(s2))
    print("Expected: 60")
    print("")
    
    print("=== LIST INLINE SUM ===")
    s3 = test_list_inline_sum()
    print("Expected: 600")
    print("")
    
    print("DONE")
end