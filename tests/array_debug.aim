# array_debug.aim
def main():
    arr = [64, 34, 25, 12, 22, 11, 90, 42, 15, 77]
    
    print("Array dump:")
    for i in range(10):
        print("arr[", i, "] =", arr[i])
    end
    
    print("Finding max:")
    max_val = arr[0]
    print("Initial max:", max_val)
    for i in range(1, 10):
        print("Checking index", i, "value", arr[i])
        if arr[i] > max_val:
            max_val = arr[i]
            print("New max:", max_val)
        end
    end
    print("Final max:", max_val)
end