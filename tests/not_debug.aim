# Debug test for 'not' operator

def main():
    print("=== NOT OPERATOR DEBUG ===")
    
    x = 5
    
    # Test 1: Simple not
    print("Test 1: not True")
    if not True:
        print("FAIL: not True should be False")
    else:
        print("PASS: not True is False")
    end
    
    # Test 2: not False
    print("Test 2: not False")
    if not False:
        print("PASS: not False is True")
    else:
        print("FAIL: not False should be True")
    end
    
    # Test 3: not (comparison)
    print("Test 3: not (x > 100) where x=5")
    if not (x > 100):
        print("PASS: not (5 > 100) is True")
    else:
        print("FAIL: not (5 > 100) should be True")
    end
    
    # Test 4: not without parens
    print("Test 4: not x > 100 where x=5")
    if not x > 100:
        print("PASS: not 5 > 100 is True")
    else:
        print("FAIL: not 5 > 100 should be True")
    end
    
    # Test 5: not (x == 5)
    print("Test 5: not (x == 5) where x=5")
    if not (x == 5):
        print("FAIL: not (5 == 5) should be False")
    else:
        print("PASS: not (5 == 5) is False")
    end
    
    # Test 6: Double not
    print("Test 6: not not True")
    if not not True:
        print("PASS: not not True is True")
    else:
        print("FAIL: not not True should be True")
    end
    
    print("=== DONE ===")
end