def main():
    # Basic concat
    a = "Hello"
    b = "World"
    c = a + " " + b
    print(c)
    
    # Literal concat
    msg = "foo" + "bar"
    print(msg)
    
    # Mixed
    name = "Claude"
    greeting = "Hello, " + name + "!"
    print(greeting)
    
    # With numbers (converted)
    prefix = "Value: "
    print(prefix)
end