# test_quick_wins.aim
# Test the quick win features we're implementing

def main():
    print("=== QUICK WINS TESTS ===")
    print("")
    
    # Test 1: pass statement
    print("TEST 1: pass statement")
    x = 5
    if x > 10:
        pass
    else:
        print("  pass in if-else works!")
    end
    
    # Test 2: None
    print("TEST 2: None")
    y = None
    if y == None:
        print("  y is None: True")
    else:
        print("  y is None: False")
    end
    
    # Test 3: Floor division //
    print("TEST 3: Floor division //")
    a = 17
    b = 5
    result = a // b
    print("  17 // 5 =", result)
    
    # Test 4: Power operator **
    print("TEST 4: Power operator **")
    base = 2
    exp = 10
    result = base ** exp
    print("  2 ** 10 =", result)
    
    # Test 5: ord()
    print("TEST 5: ord()")
    c = "A"
    code = ord(c)
    print("  ord('A') =", code)
    c2 = "a"
    code2 = ord(c2)
    print("  ord('a') =", code2)
    
    # Test 6: chr()
    print("TEST 6: chr()")
    n = 65
    ch = chr(n)
    print("  chr(65) =", ch)
    n2 = 97
    ch2 = chr(n2)
    print("  chr(97) =", ch2)
    
    # Test 7: sum()
    print("TEST 7: sum()")
    nums = [1, 2, 3, 4, 5]
    total = sum(nums)
    print("  sum([1,2,3,4,5]) =", total)
    
    print("")
    print("=== ALL QUICK WINS TESTS COMPLETE ===")
end