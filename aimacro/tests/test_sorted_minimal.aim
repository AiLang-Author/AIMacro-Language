# test_sorted_minimal.aim
# Isolate the sorted() bug

def main():
    print("Step 1: Create array")
    arr = [5, 2, 8, 1, 9, 3]
    
    print("Step 2: Print array size")
    print("Size:", len(arr))
    
    print("Step 3: Print array contents")
    for x in arr:
        print(x)
    end
    
    print("Step 4: Call sorted()")
    result = sorted(arr)
    
    print("Step 5: Print result size")
    print("Result size:", len(result))
    
    print("Step 6: Print result contents")
    for x in result:
        print(x)
    end
    
    print("Done!")
end